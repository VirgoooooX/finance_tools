  "metrics": [
    {
      "name": "流动比率",
      "formula": "safe_div(current_assets_end, current_liabilities_end)",
      "format": "percent"
    },
    {
      "name": "速动比率",
      "formula": "safe_div(current_assets_end - inventory_end, current_liabilities_end)",
      "format": "percent"
    },
    {
      "name": "现金比率",
      "formula": "safe_div(cash_end, current_liabilities_end)",
      "format": "percent"
    },
    {
      "name": "资产负债率",
      "formula": "safe_div(liabilities_total_end, assets_total_end)",
      "format": "percent"
    },
    {
      "name": "产权比率",
      "formula": "safe_div(liabilities_total_end, equity_total_end)",
      "format": "percent"
    },
    {
      "name": "权益乘数",
      "formula": "safe_div(assets_total_end, equity_total_end)",
      "format": "percent"
    },
    {
      "name": "毛利率",
      "formula": "safe_div(revenue_ytd - cost_ytd, revenue_ytd)",
      "format": "percent"
    },
    {
      "name": "营业利润率",
      "formula": "safe_div(operating_profit_ytd, revenue_ytd)",
      "format": "percent"
    },
    {
      "name": "净利率",
      "formula": "safe_div(net_profit_ytd, revenue_ytd)",
      "format": "percent"
    },
    {
      "name": "ROE(净资产收益率)",
      "formula": "safe_div(net_profit_ytd, equity_total_end)",
      "format": "percent"
    },
    {
      "name": "ROA(总资产收益率)",
      "formula": "safe_div(net_profit_ytd, assets_total_end)",
      "format": "percent"
    },
    {
      "name": "经营活动现金流净额",
      "formula": "operating_cf_ytd",
      "format": "number"
    },
    {
      "name": "投资活动现金流净额",
      "formula": "investing_cf_ytd",
      "format": "number"
    },
    {
      "name": "筹资活动现金流净额",
      "formula": "financing_cf_ytd",
      "format": "number"
    },
    {
      "name": "现金流量比率",
      "formula": "safe_div(operating_cf_ytd, current_liabilities_end)",
      "format": "percent"
    },
    {
      "name": "当月研发投入强度",
      "formula": "safe_div(rd_m, revenue_m)",
      "format": "percent"
    },
    {
      "name": "本年研发投入强度",
      "formula": "safe_div(rd_ytd, revenue_ytd)",
      "format": "percent"
    },
    {
      "name": "本年现价工业总产值",
      "formula": "(main_revenue_ytd or 0.0) + (products_end - products_begin)",
      "format": "number"
    },
    {
      "name": "应收账款周转率",
      "formula": "safe_div(revenue_ytd, ar_begin + ar_end)",
      "format": "round2"
    },
    {
      "name": "存货周转率",
      "formula": "safe_div(cost_ytd, inventory_begin + inventory_end)",
      "format": "round2"
    }
  ]
}